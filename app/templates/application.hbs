<ul class="events">
  {{#each @model.events as |event|}}
    <Event
      @title={{event.fields.name}}
      @start={{event.fields.start_time}}
      @end={{event.fields.end_time}}
      @speakers={{event.fields.speakers}}
      @images={{array "yehuda-katz" "jen-weber" "godfrey-chan"}}
    />
  {{/each}}
</ul>